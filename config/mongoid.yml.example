development:
  sessions:
   default:
     hosts:
       - localhost:27017
     database: googleplaycrawler_dev
     options:
       safe: true

test:
  sessions:
   default:
     hosts:
       - localhost:27017
     database: googleplaycrawler_test
     options:
       safe: true

production:
  sessions:
   default:
     hosts:
       - <host>
     database: googleplaycrawler_prod
     username: <username>
     password: <password>
     options:
       safe: true
